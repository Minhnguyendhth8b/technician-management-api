{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db"
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Member": {
    "dataSource": "mongodb",
    "public": true
  },
  "Notification": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "find": true,
          "seen": true,
          "readNotification": true
        }
      }
    }
  },
  "Conversation": {
    "dataSource": "mongodb",
    "public": true
  },
  "Message": {
    "dataSource": "mongodb",
    "public": false
  },
  "PSC": {
    "dataSource": "mongodb",
    "public": true
  },
  "Customer": {
    "dataSource": "mongodb",
    "public": true
  },
  "Branch": {
    "dataSource": "mongodb",
    "public": true
  }
}
